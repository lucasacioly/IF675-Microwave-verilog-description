:vpi_module "v2005_math";
:vpi_module "va_math";
S_011B8478 .scope module, "counter_tb" "counter_tb" 2 4;
 .timescale -3 -3;
v011B1608_0 .net "cdelay_tb", 0 0, v011B4620_0; 1 drivers
v011B1660_0 .var "clear_tb", 0 0;
v011B16B8_0 .var "clk_tb", 0 0;
v011B1710_0 .var/i "i", 31 0;
S_011B8830 .scope module, "DUT" "counter" 2 13, 3 1, S_011B8478;
 .timescale -3 -3;
v01032D50_0 .net "clear", 0 0, v011B1660_0; 1 drivers
v011B4570_0 .net "clk", 0 0, v011B16B8_0; 1 drivers
v011B45C8_0 .var "counter", 2 0;
v011B4620_0 .var "delay", 0 0;
E_01031E90/0 .event negedge, v01032D50_0;
E_01031E90/1 .event posedge, v011B4570_0;
E_01031E90 .event/or E_01031E90/0, E_01031E90/1;
    .scope S_011B8830;
T_0 ;
    %set/v v011B45C8_0, 0, 3;
    %end;
    .thread T_0;
    .scope S_011B8830;
T_1 ;
    %wait E_01031E90;
    %load/v 8, v011B45C8_0, 3;
    %mov 11, 0, 2;
   %cmpi/u 8, 7, 5;
    %jmp/0xz  T_1.0, 5;
    %load/v 8, v011B45C8_0, 3;
    %mov 11, 0, 29;
    %addi 8, 1, 32;
    %ix/load 0, 3, 0;
    %assign/v0 v011B45C8_0, 0, 8;
T_1.0 ;
    %movi 8, 4, 5;
    %load/v 13, v011B45C8_0, 3;
    %mov 16, 0, 2;
    %cmp/u 8, 13, 5;
    %or 5, 4, 1;
    %jmp/0xz  T_1.2, 5;
    %ix/load 0, 1, 0;
    %assign/v0 v011B4620_0, 0, 1;
    %load/v 8, v01032D50_0, 1;
    %jmp/0xz  T_1.4, 8;
    %ix/load 0, 3, 0;
    %assign/v0 v011B45C8_0, 0, 0;
T_1.4 ;
    %jmp T_1.3;
T_1.2 ;
    %ix/load 0, 1, 0;
    %assign/v0 v011B4620_0, 0, 0;
T_1.3 ;
    %jmp T_1;
    .thread T_1;
    .scope S_011B8478;
T_2 ;
    %vpi_call 2 20 "$dumpfile", "counter_tb.vcd";
    %vpi_call 2 21 "$dumpvars", 1'sb0, S_011B8478;
    %set/v v011B16B8_0, 0, 1;
    %set/v v011B1710_0, 0, 32;
T_2.0 ;
    %load/v 8, v011B1710_0, 32;
   %cmpi/s 8, 200, 32;
    %jmp/0xz T_2.1, 5;
    %delay 5, 0;
    %load/v 8, v011B16B8_0, 1;
    %inv 8, 1;
    %set/v v011B16B8_0, 8, 1;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v011B1710_0, 32;
    %set/v v011B1710_0, 8, 32;
    %jmp T_2.0;
T_2.1 ;
    %end;
    .thread T_2;
    .scope S_011B8478;
T_3 ;
    %set/v v011B1660_0, 1, 1;
    %delay 100, 0;
    %set/v v011B1660_0, 0, 1;
    %delay 100, 0;
    %set/v v011B1660_0, 1, 1;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
